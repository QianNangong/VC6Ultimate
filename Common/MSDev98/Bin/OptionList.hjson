				{
					Name:"Current Line"
					Options:[
						{
							Type:boolean
							Variable:bHighlightCurrentLine
							DefaultValue:false
							Title:"Highlight current line"
							Description:"Highlights the current line. Three styles are available:\n- Change the line's background color\n- Draw a Dotted Box around the line\n- Draw a Solid Box around the line"
							Section:"CurrentLine"
							Params:[
								{
									Type:enum
									Variable:HighlightCurrentLineType
									DefaultValue:0
									Values:[
										{Title:"Background Color", Value:0}
										{Title:"Dotted Box", Value:1}
										{Title:"Solid Box", Value:2}
									]
									Params:[
										{
											Type:color
											Variable:HighlightCurrentLineBackgroundColor
											DefaultValue:0x808080
											Condition:{
												Type:enum
												Variable:HighlightCurrentLineType
												Value:0
											}
										}
									]
								}
							]
						}
						{
							Type:boolean
							Variable:bHighlightMargin
							DefaultValue:false
							Title:"Highlight current line margin"
							Section:"CurrentLine"
							Params:[
								{
									Type:color
									Variable:CurrentLineMarginBackgroundColor
									DefaultValue:0x808080
								}
							]
						}
						{
							Type:boolean
							Variable:bUseScrollLock
							DefaultValue:false
							Title:"Always center the view on the current line"
							Section:"CurrentLine"
						}
						{
							Type:boolean
							Variable:bEndOfLineRightTrim
							DefaultValue:false
							Title:"LineEnd command ignores trailing whitespaces on first call (toggling this setting can corrupt existing macros)"
							Description:"The 'LineEnd' command first stops at the trailing whitespaces, then jumps to the end of line like the original function.\nThis behavior affects the original 'LineEnd' command so if a macro uses it, the behavior of the macro will be affected too."
							Section:"CurrentLine"
						}
						{
							Type:boolean
							Variable:bLineCutAutoToggle
							DefaultValue:false
							Title:"Automatically use LineCut instead of Cut when no text is selected"
							Section:"CurrentLine"
						}
					]
				}
